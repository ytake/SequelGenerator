{
  "name": "comnect/consoler",
  "description": "php cli application package",
  "minimum-stability": "alpha",
  "license": "MIT",
  "keywords": [
    "console",
    "php",
    "cli"
  ],
  "authors": [
    {
      "name": "yuuki.takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "symfony/console": "~2.0",
    "symfony/filesystem": "~2.0",
    "doctrine/cache": "~1.0",
    "phpoffice/phpexcel": "~1.0",
    "illuminate/container": "~5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Ytake\\SequelGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  }
}
